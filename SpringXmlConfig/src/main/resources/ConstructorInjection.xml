<?xml version="1.0" encoding="UTF-8"?>
       <beans xmlns="http://www.springframework.org/schema/beans"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xmlns:context="http://www.springframework.org/schema/context"
              xsi:schemaLocation="http://www.springframework.org/schema/beans
                   http://www.springframework.org/schema/beans/spring-beans.xsd
                   http://www.springframework.org/schema/context
                   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
                   
                   <bean class="constructorInjection.Student" id="student">
                   <constructor-arg name="name" value="Ritik Datey"></constructor-arg>
                   <constructor-arg name="course" value="Java Full-Stack" />
                   <constructor-arg name="rollNo" value="7734"></constructor-arg>
                   <constructor-arg name="fee" value="35000"></constructor-arg>
                   </bean>
	
        </beans>